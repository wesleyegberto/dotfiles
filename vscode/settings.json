  "vim.statusBarColorControl": false,
  "vim.searchHighlightColor": "rgba(35, 204, 176, 0.8)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.color": "rgba(255, 170, 26, 0.5)",
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.sneak": false,
  "vim.gdefault": true,
  "vim.leader": " ",
  "vim.handleKeys": {
    "<C-s>": false,
    "<C-n>": false
  },
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-s>"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<Leader>", "<Esc>"],
      "commands": [":nohl"]
    },
    {
      "before": ["<C-w>", ">"],
      "commands": ["workbench.action.increaseViewSize"]
    },
    {
      "before": ["<C-w>", "<"],
      "commands": ["workbench.action.decreaseViewSize"]
    },
    {
      "before": ["<C-w>", "z"],
      "commands": ["workbench.action.joinAllGroups"]
    },
    {
      "before": ["<C-w>", "<S-r>"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"]
    },
    {
      "before": ["<C-w>", "r"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<C-w>", "t"],
      "commands": ["workbench.action.toggleEditorWidths"]
    },
    {
      "before": ["<C-w>", "m"],
      "commands": ["workbench.action.minimizeOtherEditors"]
    },
    {
      "before": ["<C-w>", "M"],
      "commands": ["workbench.action.maximizeEditor"]
    },
    {
      "before": ["<C-w>", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<C-w>", "D"],
      "commands": ["workbench.action.closeEditorsInGroup"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "e"],
      "commands": ["workbench.action.toggleSidebarVisibility"]
    },
    {
      "before": ["<leader>", "y"],
      "commands": ["workbench.files.action.showActiveFileInExplorer"]
    },
    {
      "before": ["<leader>", "w", "c"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "w", "f"],
      "commands": ["workbench.action.toggleEditorGroupLayout"]
    },
    {
      "before": ["<leader>", "w", "v"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "w", "s"],
      "commands": ["workbench.action.splitEditorOrthogonal"]
    },
    {
      "before": ["<leader>", "w", "d"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "w", "u"],
      "commands": ["workbench.action.reopenClosedEditor"]
    },
    {
      "before": ["<leader>", "w", "m"],
      "commands": ["editor.action.showContextMenu"]
    },

    // files
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<leader>", "f", "b"],
      "commands": ["workbench.action.showAllEditors"]
    },
    {
      "before": ["<leader>", "f", "r"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["g", "b"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["g", "B"],
      "commands": ["workbench.action.previousEditor"]
    },

    // source file
    {
      "before": ["<leader>", "c", "s", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["<leader>", "c", "s", "i"],
      "commands": ["editor.action.indentationToTabs"]
    },
    {
      "before": [">", ">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<", "<"],
      "commands": ["editor.action.outdentLines"]
    },

    // marks
    {
      "before": ["<leader>", "f", "m"],
      "commands": ["bookmarks.list"]
    },
    {
      "before": ["<leader>", "f", "<S-m>"],
      "commands": ["bookmarks.listFromAllFiles"]
    },
    {
      "before": ["<leader>", "m", "<S-m>"],
      "commands": ["bookmarks.listFromAllFiles"]
    },
    {
      "before": ["<leader>", "m", "a"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "m", "t"],
      "commands": ["bookmarks.toggle"]
    },
    {
      "before": ["<leader>", "m", "<S-c>"],
      "commands": ["bookmarks.clearFromAllFiles"]
    },
    {
      "before": ["<leader>", "m", "n"],
      "commands": ["bookmarks.jumpToNext"]
    },
    {
      "before": ["<leader>", "m", "p"],
      "commands": ["bookmarks.jumpToPrevious"]
    },


    // git
    {
      "before": ["<leader>", "g", "p"],
      "commands": ["git.pullRebase"]
    },
    {
      "before": ["<leader>", "g", "r"],
      "commands": ["git.rebase"]
    },
    {
      "before": ["<leader>", "g", "f"],
      "commands": ["git.fetchAll"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": ["gitlens.toggleLineBlame"]
    },
    {
      "before": ["<leader>", "g", "B"],
      "commands": ["gitlens.toggleFileBlame"]
    },
    {
      "before": ["<leader>", "g", "M"],
      "commands": ["git.merge"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["gitlens.showQuickRepoStatus"]
    },
    {
      "before": ["<leader>", "g", "a"],
      "commands": ["git.stage"]
    },
    {
      "before": ["<leader>", "g", "A"],
      "commands": ["git.stageAll"]
    },
    {
      "before": ["<leader>", "g", "u"],
      "commands": ["git.unstage"]
    },
    {
      "before": ["<leader>", "g", "U"],
      "commands": ["git.unstageAll"]
    },
    {
      "before": ["<leader>", "g", "c"],
      "commands": ["git.commit"]
    },
    {
      "before": ["<leader>", "g", "P"],
      "commands": ["git.push"]
    },
    {
      "before": ["<leader>", "g", "l"],
      "commands": ["git.viewHistory"]
    },
    {
      "before": ["<leader>", "g", "h"],
      "commands": ["git.viewLineHistory"]
    },
    {
      "before": ["<leader>", "g", "H"],
      "commands": ["git.viewFileHistory"]
    },

    {
      "before": ["<leader>", "g", "d", "f"],
      "commands": ["gitlens.diffWithPreviousInDiffLeft"]
    },
    {
      "before": ["<leader>", "g", "d", "c"],
      "commands": ["workbench.action.closeActiveEditor"]
    },

    // diagnostic navigation
    {
      "before": ["]", "g"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["[", "g"],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["g", ";"],
      "commands": ["workbench.action.navigateToLastEditLocation"]
    },
    {
      "before": ["]", "c"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["[", "c"],
      "commands": ["workbench.action.editor.previousChange"]
    },
    {
      "before": ["]", "m", "c", ],
      "commands": ["merge.goToNextConflict"]
    },
    {
      "before": ["[", "m", "c"],
      "commands": ["merge.goToPreviousConflict"]
    },

    // definitions and references
    {
      "before": ["g", "d"],
      "commands": ["editor.action.goToDeclaration"]
    },
    {
      "before": ["g", "v", "d"],
      "commands": ["editor.action.revealDefinitionAside"]
    },
    {
      "before": ["g", "D"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["g", "i"],
      "commands": ["editor.action.goToImplementation"]
    },
    {
      "before": ["g", "I"],
      "commands": ["editor.action.peekImplementation"]
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.goToReferences"]
    },
    {
      "before": ["<leader>", "C"],
      "commands": ["editor.action.commentLine"],
      "when": "editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<leader>", "c", "a", "l"],
      "commands": ["editor.action.sourceAction"]
    },
    {
      "before": ["<leader>", "c", "q", "f"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["<leader>", "c", "r", "n"],
      "commands": ["editor.action.rename"],
      "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
      "before": ["<leader>", "c", "r", "a"],
      "commands": ["editor.action.refactor"]
    },
    {
      "before": ["<leader>", "c", "h"],
      "commands": ["editor.action.triggerParameterHints"]
    },

    // run
    {
      "before": ["<leader>", "r", "t", "a"],
      "commands": ["testing.runAll"]
    },
    {
      "before": ["<leader>", "r", "t", "c"],
      "commands": ["testing.runAtCursor"]
    },
    {
      "before": ["<leader>", "r", "t", "f"],
      "commands": ["testing.runCurrentFile"]
    },
    {
      "before": ["<leader>", "r", "t", "l"],
      "commands": ["testing.reRunLastRun"]
    },
    {
      "before": ["<leader>", "r", "t", "F"],
      "commands": ["testing.reRunFailTests"]
    },
    {
      "before": ["<leader>", "r", "t", "o"],
      "commands": ["testing.openOutputPeek"]
    },
    {
      "before": ["<leader>", "r", "t", "v"],
      "commands": ["workbench.view.testing.focus"]
    },

    // terminal
    {
      "before": ["<leader>", "t", ";"],
      "commands": ["workbench.action.terminal.newInActiveWorkspace"]
    },
    {
      "before": ["<leader>", "t", "t"],
      "commands": ["workbench.action.togglePanel"]
    },
    {
      "before": ["<leader>", "t", "g"],
      "commands": ["workbench.action.terminal.newInActiveWorkspace"]
    },
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "C"],
